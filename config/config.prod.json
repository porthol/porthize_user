{
  "templateexpress": {
    "databases": [
      {
        "host": "mongo1",
        "port": 27017
      },
      {
        "host": "mongo2",
        "port": 27017
      },
      {
        "host": "mongo3",
        "port": 27017
      }
    ],
    "databaseName": "user",
    "traefik": {
      "gatewayAddress": "gateway",
      "gatewayPort": 80
    },
    "rabbitmq": {
      "url": "amqp",
      "port": 5672,
      "user": "guest",
      "password": "guest",
      "options": {
        "queueName": "notification-service",
        "exchangeName": "",
        "enableAMQPLogger": false,
        "autoDelete": true,
        "messageTTL": 60000,
        "noAck": true,
        "reconnectTime": 10000
      }
    },
    "authorizationService": {
      "name": "user",
      "addRoute": "privileges/{resource}/routes",
      "authorizationRoute": "users/isAuthorized",
      "authenticationRoute": "token",
      "registerAppRoute": "users/registerMicroService",
      "renewTokenRoute": "users/renewBotToken",
      "rolePrivilegeRoute": "roles/importPrivileges"
    },
    "checkBotAccountTime": "48h"
  }
}
